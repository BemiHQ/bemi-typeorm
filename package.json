{
  "name": "@bemi-db/typeorm",
  "version": "1.1.0",
  "description": "Automatic database change tracking for TypeORM.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "bin": {
    "bemi": "./dist/cli.js"
  },
  "repository": "git://github.com/BemiHQ/bemi-typeorm.git",
  "author": "",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/BemiHQ/bemi-typeorm/issues"
  },
  "homepage": "https://github.com/BemiHQ/bemi-typeorm#readme",
  "dependencies": {
    "express": "^4.18.2",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "@types/sinon": "^17.0.2",
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "commander": "^11.1.0",
    "mocha": "^10.2.0",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "^5.0.0"
  },
  "registry-url": "https://registry.npmjs.org/",
  "scripts": {
    "build": "tsup",
    "test": "mocha -r ts-node/register './test/*.test.ts'"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/cli.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
